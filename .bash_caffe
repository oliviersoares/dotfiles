#!/usr/bin/env bash

export CAFFE_ROOT=${HOME}/dev/build/caffe
if [ "$(uname)" == "Darwin" ]; then
  export DYLD_LIBRARY_PATH=${DYLD_LIBRARY_PATH}:${CAFFE_ROOT}/lib
else
  export LD_LIBRARY_PATH=${LD_LIBRARY_PATH}:${CAFFE_ROOT}/lib
fi
export PYTHONPATH=${PYTHONPATH}:${CAFFE_ROOT}/python
export PATH=${CAFFE_ROOT}/bin:${PATH}
